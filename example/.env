# App configuration
VITE_APP_NAME="My Awesome App"
# This is the version of the app
VITE_APP_VERSION=1.0.0
# Set DEBUG to true for development
VITE_APP_DEBUG=true
# The base URL for the app
VITE_APP_URL="https://example.com"

# Database configuration
VITE_APP_DB_HOST="localhost"
# Database port number
VITE_APP_DB_PORT=5432
# Database username
VITE_APP_DB_USER="db_user"
# Database password (ensure it is strong)
VITE_APP_DB_PASSWORD="P@ssw0rd!"
# Database name
VITE_APP_DB_NAME="my_database"

# API keys
VITE_APP_API_KEY="your_api_key"
# Secret key for sensitive operations
VITE_APP_SECRET_KEY="your_secret_key"

# Feature toggles
# Enable or disable Feature Flag 1
VITE_APP_FEATURE_FLAG_1=true
# Enable or disable Feature Flag 2
VITE_APP_FEATURE_FLAG_2=false

# External services
# URL for an external API
VITE_APP_EXTERNAL_API_URL="https://api.example.com"
# API key for the external service
VITE_APP_EXTERNAL_API_KEY="external_api_key"

# Social media
# Facebook App ID for social login
VITE_APP_SOCIAL_FACEBOOK_APP_ID="facebook_app_id"
# Twitter API key for social sharing
VITE_APP_SOCIAL_TWITTER_API_KEY="twitter_api_key"

# Notification settings
# Email address for admin notifications
VITE_APP_NOTIFICATION_EMAIL="admin@example.com"
# Slack webhook for important alerts
VITE_APP_NOTIFICATION_SLACK_WEBHOOK="https://slack.com/webhook"

# JSON data
# Example JSON data for configuration
VITE_APP_JSON_DATA={"key1":"value1","key2":"value2"}

# Nonstandard json data
# Example JSON data for configuration
VITE_APP_NONSTANDARD_JSON_DATA={key1:'value1',key2:"value2"}

# Array data
# Example array data
VITE_APP_ARRAY_DATA=['item1', 'item2', 'item3']

# Multiline data
# This is a multiline value.
# You can break it into multiple lines.
# It will be concatenated.
VITE_APP_MULTILINE_DATA="This is a multiline value. \
You can break it into multiple lines \
and it will be concatenated."
